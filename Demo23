// ABC : A,B,C,AB,BC,ABC
class Demo23{
    public static void main(String[] args){
        String str = "ABC";
        System.out.println("String : "+str);
        for(int i=0;i<str.length();i++){
            int m=0;
            for(int j=0;j<str.length();j++){
                for(m=j;m<=i+j;m++){
                System.out.print(str.charAt(m));
                }
                System.out.println();
                if(m==str.length())
                break;
            }
        }
    }
}